{
  "name": "vest",
  "publisher": "AdriHM",
  "displayName": "vest",
  "icon": "logo.png",
  "repository": "https://github.com/ady642/vest",
  "description": "utility to generate test suites for a vue component",
  "version": "1.1.6",
  "engines": {
    "vscode": "^1.65.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:vest.generateTestSuites",
    "onStartupFinished:vest.activateCodeLens"
  ],
  "main": "out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "vest.generateTestSuites",
        "title": "Vest - Generate unit tests"
      }
    ]
  },
  "keybindings": [
    {
      "command": "vest.generateTestSuites",
      "key": "ctrl+shift+,",
      "mac": "shift+cmd+,"
    }
  ],
  "scripts": {
    "dev": "nodemon",
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node out/test/runTest.js"
  },
  "devDependencies": {
    "@types/eslint": "^8.4.1",
    "@types/glob": "^7.2.0",
    "@types/jest": "^27.4.1",
    "@types/mkdirp": "^1.0.2",
    "@types/mocha": "^9.1.0",
    "@types/node": "14.x",
    "@types/vscode": "^1.65.0",
    "@typescript-eslint/eslint-plugin": "^5.12.1",
    "@typescript-eslint/parser": "^5.12.1",
    "@vscode/test-electron": "^2.1.2",
    "eslint": "^8.9.0",
    "glob": "^7.2.0",
    "jest": "^27.5.1",
    "mocha": "^9.2.1",
    "nodemon": "^2.0.15",
    "ts-jest": "^27.1.4",
    "ts-node": "^10.7.0",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.5.5"
  },
  "dependencies": {
    "babel-jest": "^27.5.1",
    "mkdirp": "^1.0.4"
  }
}
